\ProvidesPackage{listingstyles/listings-command}

% Custom language style definition
\lstdefinestyle{command-style}{
    language=csh,
    frame=single,
    numbers=left,
    numbersep=5pt,
    basicstyle=\ttfamily\small,
    showstringspaces=false,
    tabsize=4,
    breaklines=true,        % enable wrapping
    breakatwhitespace=false, % break only at spaces
    breakautoindent=true
}

% Custom language style definition
\lstdefinestyle{command-dense-style}{
    language=csh,
    frame=single,
    numbers=left,
    numbersep=5pt,
    basicstyle=\ttfamily,
    showstringspaces=false,
    tabsize=4,
    breaklines=true,        % enable wrapping
    breakatwhitespace=true, % break only at spaces
    breakautoindent=true
}